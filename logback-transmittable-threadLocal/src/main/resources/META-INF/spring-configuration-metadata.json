{
  "properties": [
    {
      "name": "thread.corePoolSize",
      "type": "java.lang.Integer",
      "description": "核心线程数",
      "defaultValue": 5
    },
    {
      "name": "thread.maxPoolSize",
      "type": "java.lang.Integer",
      "description": "最大线程数",
      "defaultValue": 20
    },
    {
      "name": "thread.queueCapacity",
      "type": "java.lang.Integer",
      "description": "队列大小",
      "defaultValue": "Integer.MAX_VALUE"
    },
    {
      "name": "thread.keepAliveSeconds",
      "type": "java.lang.Integer",
      "description": "线程活跃时间(秒)",
      "defaultValue": 60
    },
    {
      "name": "thread.waitForJobsToCompleteOnShutdown",
      "type": "java.lang.Boolean",
      "description": "是否所有任务结束之后再关闭线程",
      "defaultValue": true
    },
    {
      "name": "thread.threadNamePrefix",
      "type": "java.lang.String",
      "description": "默认线程池名称",
      "defaultValue": ""
    }
  ]
}